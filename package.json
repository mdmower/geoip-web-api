{
  "name": "geoip-web-api",
  "version": "3.0.3",
  "description": "An AMP-GEO compatible location web API",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile",
    "clean": "rimraf lib",
    "compile": "tsc",
    "compile:debug": "tsc --build tsconfig.dev.json",
    "dev": "tsx --watch src/cli.ts --config config.dev.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "prepare": "npm run clean && npm run compile",
    "start": "node lib/cli.js",
    "test": "jest"
  },
  "engines": {
    "node": ">= 18.19.0"
  },
  "bin": {
    "geoip-web-api": "lib/cli.js"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/geoip-web-api.git"
  },
  "keywords": [
    "geoip",
    "amp-geo",
    "location",
    "maxmind"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/geoip-web-api/issues"
  },
  "homepage": "https://github.com/mdmower/geoip-web-api#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transform": {
      "\\.[jt]s$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "dependencies": {
    "express": "^4.21.1",
    "ip2ldb-reader": "^3.0.2",
    "maxmind": "^4.3.22",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.17.6",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.15.0"
  }
}
