{
  "name": "geoip-web-api",
  "version": "2.0.1",
  "description": "An AMP-GEO compatible location web API",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile",
    "build-release": "npm run lint && npm run clean && npm run compile-release",
    "clean": "rimraf lib",
    "compile": "tsc --build tsconfig.json",
    "compile-release": "tsc --build tsconfig.release.json",
    "debug": "node --inspect-brk lib/cli.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint src",
    "prepublishOnly": "npm run build-release",
    "start": "node lib/cli.js",
    "test": "jest"
  },
  "bin": {
    "geoip-web-api": "lib/cli.js"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/geoip-web-api.git"
  },
  "keywords": [
    "geoip",
    "amp-geo",
    "location",
    "maxmind"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/geoip-web-api/issues"
  },
  "homepage": "https://github.com/mdmower/geoip-web-api#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ]
  },
  "dependencies": {
    "express": "^4.17.2",
    "ip2ldb-reader": "^2.0.1",
    "maxmind": "^4.3.3",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsdoc": "^37.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  }
}
