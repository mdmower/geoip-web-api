{
  "name": "geoip-web-api",
  "version": "3.0.1",
  "description": "An AMP-GEO compatible location web API",
  "type": "module",
  "scripts": {
    "build": "npm run lint && npm run clean && npm run compile",
    "build-release": "npm run lint && npm run clean && npm run compile-release",
    "clean": "rimraf lib",
    "compile": "tsc --build tsconfig.json",
    "compile-release": "tsc --build tsconfig.release.json",
    "debug": "node --inspect-brk lib/cli.js",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint .",
    "prepare": "npm run clean && npm run compile-release",
    "start": "node lib/cli.js",
    "test": "jest"
  },
  "engines": {
    "node": ">= 18.19.0"
  },
  "bin": {
    "geoip-web-api": "lib/cli.js"
  },
  "files": [
    "/lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdmower/geoip-web-api.git"
  },
  "keywords": [
    "geoip",
    "amp-geo",
    "location",
    "maxmind"
  ],
  "author": "Matt Mower <mdmower@cmphys.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mdmower/geoip-web-api/issues"
  },
  "homepage": "https://github.com/mdmower/geoip-web-api#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/"
    ],
    "transform": {
      "\\.[jt]s$": [
        "ts-jest",
        {
          "useESM": true,
          "tsconfig": "tsconfig.json"
        }
      ]
    },
    "moduleNameMapper": {
      "(.+)\\.js": "$1"
    },
    "extensionsToTreatAsEsm": [
      ".ts"
    ]
  },
  "dependencies": {
    "express": "^4.21.0",
    "ip2ldb-reader": "^3.0.1",
    "maxmind": "^4.3.21",
    "minimist": "^1.2.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.16.10",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "eslint-plugin-jsdoc": "^50.3.0",
    "eslint-plugin-prettier": "^5.2.1",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "~5.5.4",
    "typescript-eslint": "^8.7.0"
  }
}
